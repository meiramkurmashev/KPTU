<form method="post" action="/directory/ConsumersLoad">
    @{
        //  Layout = null;//для представления не используется мастер-страница
    }<br />
    <table>
        <tr><td><b><label>Мин-во</label></b></td><td><input name=id1 required></td></tr>
        <tr><td><b><label>Респ.</label></b></td><td><input name=id2 required></td></tr>
        <tr><td><b><label>Порядк.</label></b></td><td><input name=id3 required></td></tr>

        <tr>
            <td><label>Наименование</label></td>
            <td>
                <input name=nm required>
            </td>
        </tr>
        <tr>
            <td><label>English name</label></td>
            <td>
                <input name=nme required>
            </td>
        </tr>

        <tr>
            <td><b><label>Ж/д станция</label></b></td>
            <td>
                <b>
                    <select required id="siF7" name="id4" onchange="update(event.target, 'bdF18')">
                        <option value="" selected>Выберите станцию</option>
                        @foreach (var wag2 in ViewBag.aw)
                        {


                            <option value="@wag2.Id">@wag2.Id - @wag2.Name</option>


                        }
                    </select>
            </td>
        </tr>
        <tr>
            <td><b><label>Группа</label></b></td>
            <td>
                <b>
                    <select required id="siF7" name="id5" onchange="update(event.target, 'bdF18')">
                        <option value="" selected>Выберите группу</option>
                        @foreach (var wag1 in ViewBag.b)
                        {


                            <option value="@wag1.Id">@wag1.Id - @wag1.Name</option>


                        }
                    </select>
            </td>
        </tr>
        <tr><td><b><label>Код потребителя 4</label></b></td><td><input name=id6 required></td></tr>
        <tr><td><b><label>Адрес потребителя</label></b></td><td><input name=adres required></td></tr>

    </table>


    @{

    }
    <button type="submit" value="">Добавить</button>
    <button type="button" onclick="javascript:history.go(-1);">Назад</button>

</form>

